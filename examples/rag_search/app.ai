app is "rag-search":
  entry_page is "home"

page is "home" at "/":
  section "upload":
    heading "RAG Search"
    text "This skeleton shows where upload/query UI would go once wired to flows."
  section "query":
    text "Use Studio to run flows manually; UI wiring is kept minimal for stability."

model "default":
  provider "dummy"

ai is "rewrite":
  model "default"
  input from "user_input"

flow is "answer":
  step is "rewrite":
    kind "ai"
    target "rewrite"
  step is "echo":
    kind "tool"
    target "echo"
