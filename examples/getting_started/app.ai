app is "getting-started":
  entry_page is "home"

page is "home" at "/":
  section "hero":
    heading "Getting Started with Namel3ss"
    text "This page uses the modern English-style DSL."

model "default":
  provider "dummy"

ai is "greet":
  model "default"
  input from "user_input"

flow is "pipeline":
  step is "call":
    kind is "ai"
    target "greet"
